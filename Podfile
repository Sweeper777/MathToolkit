# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

abstract_target 'defaults' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MathToolkit
  pod 'SCLAlertView', :git => 'https://github.com/ahmedsafadii/SCLAlertView-Swift.git'
  pod 'EZLoadingActivity'
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'SwiftyUtils'
  pod 'SwiftRandom'
  pod 'DDMathParser', :git => 'https://github.com/davedelong/DDMathParser.git', :branch => 'swift4'
  pod 'MGSwipeTableCell'
  pod 'Regexer'
  pod 'Eureka'
  pod 'ViewRow', :git => 'https://github.com/EurekaCommunity/ViewRow'
  pod 'RFKeyboardToolbar'
  
  target 'MathToolkit'
  target 'MathToolkitTests'
end
